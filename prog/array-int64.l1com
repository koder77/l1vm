// array demo
#include <intr-func.l1h>

(main func)
	(set int64 1 zero 0)
	(set int64 1 one 1)
	(set int64 1 offset 8)
	(set int64 1 x 23)
	(set int64 1 y 42)
	(set int64 1 a 5)
	(set int64 1 b 10)
	(set int64 2 z 0 0)

	(x :print_i !)
	(:print_n !)

	// assign to array
	(:assign_to_array)
	(x z [ zero ] =)
	// get array variable
	(:read-from-array)
	(reset-reg)
	(z [ zero ] a =)
    (reset-reg)
	((a zero +) a =)
	(a :print_i !)
	(:print_n !)

	(zero :exit !)
(funcend)
