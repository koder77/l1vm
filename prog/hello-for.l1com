// hello-for - Brackets - Hello world! do loop
//
#include <intr.l1h>
(main func)
	(set int64 1 zero 0)
	(set int64 1 one 1)
	(set int64 1 loop 0)
	(set int64 1 maxloop 10)
	(set int64 1 f 0)
	// set string length automatic, by setting "s"
	(set string s hello "Hello world!")
	// print string
	// for
	(zero loop =)
	(for-loop)
	(((loop maxloop <) f =) f for)
        print_s (hello)
		// print newline
        print_n
		((loop one +) loop =)
	(next)
	exit (zero)
(funcend)
