// hello-func.l1com
//
// new macro as functions example!
//
#include <intr.l1h>
(main func)
	(set int64 1 zero 0)
	(set int64 1 x 23)
	(set int64 1 y 42)
	(set int64 1 a 0)
	(set string 13 hello "Hello world!")
	// print string
	(hello :print_s !)
	(:print_n !)
	((x y *) a =)
    (a :print_i !)
	(:print_n !)
	exit (zero)
(funcend)
