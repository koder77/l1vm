#include <intr.l1h>
#include <bool.l1h>
(main func)
    (set int64 1 zero 0)
    (set int64 1 one 1)
    (set int64 1 x1 5)
    (set int64 1 y1 3)

    (set int64 1 x2 -5)
    (set int64 1 y2 3)

    (set int64 1 ret 0)
    (set int64 1 f 0)
    (set int64 1 retcode 0)
    // run tests

    // test 1
    (x1 y1 :multiply !)
    (ret stpop)

    print_i (ret)
    print_n

    // test 2
    (x2 y2 :multiply !)
    (ret stpop)

    print_i (ret)
    print_n

    exit (retcode)
(funcend)
#include <multiply.l1h>
