// hello.l1com
// Brackets - Hello world!
//
#include <intr-func.l1h>
(main func)
	(set int64 1 zero 0)
	(set int64 1 x 23)
	(set int64 1 y 42)
	(set int64 1 a 0)
	(set string 13 hello "Hello world!")
	// print string
	(hello :print_s !)
	(:print_n !)
	((x y *) a =)
	(a :print_i !)
	(:print_n !)
	(zero :exit !)
(funcend)
