# -*- mode: snippet -*-
# name: L1VM Function with Contracts
# key: funccon
# --
($1 func)
    #var ~ $1
    (set const-int64 1 zero~ 0)
    (set const-int64 1 one~ 1)
    (set int64 1 ret~ 0)

    (precondition)
    $2
    (precondition-end)

    $0

    (postcondition)
    $3
    (postcondition-end)

    (ret~ zero~ stpush) (return)
(funcend)
