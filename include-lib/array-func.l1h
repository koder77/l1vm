// array-func.l1h

// map function, gets label and pushes arguments, then executes FUNC_LABEL (function pointer) function
#func map (FUNC_LABEL, PA, PB, PC) :(PA PB PC stpush)@#(FUNC_LABEL jsra)

// reduce function, gets array range and the arrays
#func reduce (FUNC_LABEL, START, END, PA, PB, PC) :(START END PA PB PC stpush)@#(FUNC_LABEL jsra)

// mapcar
#func mapcar (FUNC_LABEL, PA, PB, PC) :(PA PB PC stpush)@#(FUNC_LABEL jsra)
